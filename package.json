{
  "name": "webpack-config",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --env mode=development",
    "build:dev": "webpack --env mode=development",
    "build:prod": "webpack --env mode=production",
    "build:prod:analyzer": "yarn run build:prod -- --env analyzer=true",
    "prettier": "npx prettier --write **/*.{ts,tsx}",
    "eslint": "eslint src/**/*.{js,jsx,ts,tsx,json}",
    "eslint:fix": "eslint --fix ' src/**/*.{js,jsx,ts,tsx,json}'",
    "stylelint:css": "npx stylelint \"**/*.css\"",
    "stylelint:css:fix": "npx stylelint \"**/*.css\" --fix"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "npx prettier --write",
      "eslint --fix"
    ],
    "src/**/*.{js,jsx,ts,tsx,json}": [
      "eslint --fix"
    ],
    "**/*.css": [
      "npx stylelint --fix"
    ]
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.23.3",
    "@babel/preset-react": "7.23.3",
    "@babel/preset-typescript": "7.23.3",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
    "@svgr/webpack": "8.1.0",
    "@types/node": "20.9.1",
    "@types/react": "18.2.37",
    "@types/react-dom": "18.2.15",
    "@types/webpack": "5.28.5",
    "@types/webpack-bundle-analyzer": "4.6.3",
    "@types/webpack-dev-server": "4.7.2",
    "@typescript-eslint/eslint-plugin": "6.14.0",
    "@typescript-eslint/parser": "6.14.0",
    "babel-loader": "9.1.3",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "6.8.1",
    "eslint": "8.55.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.0.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "fork-ts-checker-webpack-plugin": "9.0.2",
    "html-webpack-plugin": "5.5.3",
    "husky": "8.0.3",
    "lint-staged": "15.2.0",
    "mini-css-extract-plugin": "2.7.6",
    "prettier": "3.1.1",
    "react-refresh": "0.14.0",
    "style-loader": "3.3.3",
    "stylelint": "16.0.2",
    "stylelint-config-clean-order": "5.2.0",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "webpack": "5.89.0",
    "webpack-bundle-analyzer": "4.10.1",
    "webpack-cli": "5.1.4",
    "webpack-dev-server": "4.15.1"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.19.0"
  }
}
